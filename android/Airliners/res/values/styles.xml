<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AircraftItemShortAttributeText" parent="AircraftItemAttributeText">
        <item name="android:textSize">12sp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="AircraftItemAttributeText">
        <item name="android:textColor">#fff</item>
        <item name="android:textColorHighlight">#fff</item>
    </style>

    <style name="AircraftItemAttributesGroupHeader">
        <item name="android:paddingTop">1dp</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:paddingRight">3dp</item>
        <item name="android:background">@drawable/bordered_block_background</item>
        <item name="android:lines">1</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textColorHighlight">#fff</item>
    </style>

    <style name="AlbumNavigationBarButton">
        <item name="android:layout_width">32dp</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:layout_marginRight">12dp</item>
    </style>
</resources>